{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "routes/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/journeybuilder/execute",
      "dest": "/routes/execute.js"
    },
    {
      "src": "/journeybuilder/save",
      "dest": "/routes/save.js"
    },
    {
      "src": "/journeybuilder/publish",
      "dest": "/routes/publish.js"
    },
    {
      "src": "/journeybuilder/validate",
      "dest": "/routes/validate.js"
    },
    {
      "src": "/journeybuilder/stop",
      "dest": "/routes/stop.js"
    },
    {
      "src": "/journeybuilder/ui/index.html",
      "dest": "/public/journeybuilder/ui/index.html"
    }
  ],
  "headers": [
    {
      "source": "/journeybuilder/*",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
